FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian; // Newtonian fluid model in which the viscosity is constant. nu=cnt

nu              [0 2 -1 0 0 0 0] 1.46e-05; // Dimensions and kinematic viscosity of the air.

